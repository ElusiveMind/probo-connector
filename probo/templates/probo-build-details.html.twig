<h2 class="probo-header">{{ build.pull_request_name }}</h2>
<div id="task-items">
  {% for task in tasks %}
  <div class="task-line-item">
    <a href="/probo/build/task/{{ build.bid }}/{{ task.tid }}">{{ task.plugin }} - {{ task.event_name }}</a> - {{ task.duration }} - Date: {{ task.date }}
  </div>
  {% endfor %}
</div>